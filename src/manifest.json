{
    "name": "Steam Extensions",
    "description": "Steam消费历史CSGO开箱数据统计--------欢迎光临直播间：https://live.bilibili.com/22447482",
    "version": "1.2.1",
    "manifest_version": 2,
    "content_scripts": [{
        "matches": [
            "*://store.steampowered.com/account/history/*"
        ],
        "js": [
            "lib/jquery-1.11.1.min.js",
            "lib/bridge.js",
            "lib/utils.js",
            "case.js"
        ],
        "css": [
            "css/case.css"
        ]
    }],
    "background": {
        "scripts": [
            "background.js"
        ]
    },
    "permissions": [
        "*://store.steampowered.com/account/history/*",
        "webRequest"
    ]
}